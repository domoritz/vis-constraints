    "postrun": "cat vl.json && vl2png vl.json > vl.png && open vl.png"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "compassql": "^0.11.0",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.6",
    "vega": "^3.0.0-beta.25",
    "vega-lite": "^2.0.0-alpha.6",
    "yargs": "^7.0.1"
  }
}
